{
    "latest": {
        "image": "nginx:latest",
        "compose": {
            "ports": [
                "80:80"
            ],
            "volumes": [
                "./conf/nginx.conf:/etc/nginx/nginx.conf",
                "./:/var/www/html"
            ]
        },
        "publishes": {
            "laravel.conf": {
                "source": "conf/{version}/laravel.conf",
                "destination": "/etc/nginx/conf.d/default.conf"
            },
            "node.conf": {
                "source": "conf/{version}/node.conf",
                "destination": "/etc/nginx/conf.d/node.conf"
            }
        }
    }
}