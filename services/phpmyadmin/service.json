{
    "latest":  {
        "image":"phpmyadmin",
        "compose":{
            "ports":[
                "${PHPMYADMIN_PORT:-8080}:80"
            ],
            "environment":  [
                "PMA_ARBITRARY: 0",
                "PMA_HOST: ${DB_HOST:-mysql}",
                "PMA_USER: ${DB_USERNAME:-root}",
                "PMA_PASSWORD: ${DB_PASSWORD:-root}",
                "MYSQL_ROOT_PASSWORD: ${DB_PASSWORD:-root}",
                "UPLOAD_LIMIT: 100M"
            ],
            "depends_on": [
                "mysql"
            ]
        }
    }
}