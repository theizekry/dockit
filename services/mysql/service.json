{
    "8.0": {
        "image": "mysql:8.0",
        "template": "docker-compose-snippet.j2",
        "requires": {
            "volumes": true,
            "ports": true,
            "environment": true
        },
        "compose": {
            "restart": "always",
            "volumes": ["mysql_data:/var/lib/mysql"],
            "ports": ["3306:3306"],
            "environment": [
                "MYSQL_ROOT_PASSWORD=root",
                "MYSQL_DATABASE=app_db",
                "MYSQL_USER=app_user",
                "MYSQL_PASSWORD=app_pass"
            ],
            "networks": ["docknet"]
        }
    }
} 