{
    "latest": {
        "image": "mysql:latest",
        "compose": {
            "volumes": [
                "./dockit/data/mysql/:/var/lib/mysql/"
            ],
            "environment": [
                "MYSQL_ROOT_PASSWORD: ${DB_PASSWORD:-root}",
                "MYSQL_DATABASE: ${DB_DATABASE:-dockit}"
            ]
        }
    }
}